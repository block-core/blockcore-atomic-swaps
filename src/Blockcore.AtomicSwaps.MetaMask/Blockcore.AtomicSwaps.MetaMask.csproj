<Project Sdk="Microsoft.NET.Sdk.Razor">

	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<Nullable>enable</Nullable>
		<WarningsAsErrors>nullable</WarningsAsErrors>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<IncludeSymbols>True</IncludeSymbols>
		<Authors>Blockcore</Authors>
		<Version>1.0.0</Version>
		<Product>Blockcore.AtomicSwaps.MetaMask</Product>
		<PackageProjectUrl></PackageProjectUrl>
		<RepositoryUrl></RepositoryUrl>
		<RepositoryType>github</RepositoryType>
		<PackageTags>metamask ethereum blockchain web3</PackageTags>
		<Description>Blockcore.AtomicSwaps.MetaMask Extension</Description>
	</PropertyGroup>


	<ItemGroup>
		<SupportedPlatform Include="browser" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.AspNetCore.Components.Web" Version="6.0.8" />
		<PackageReference Include="Nethereum.ABI" Version="4.7.0" />
		<PackageReference Include="Nethereum.RPC" Version="4.7.0" />
	</ItemGroup>

</Project>
